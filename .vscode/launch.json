  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "targetArchitecture": "arm64",
      "program": "${workspaceFolder}/bazel-bin/nullspaces/hand_runner",
      "args": [],
      "preLaunchTask": "build hand_tracker",
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        },
        {
          "description": "substition path",
          "text": "set substitute-path /proc/self/cwd ${workspaceFolder}",
        },
        {
          "description": "pretty",
          "text": "python import sys; sys.path.insert(0, '/home/radxa/gcc/install/share/gcc-15.1.0/python')"
        },
        {
          "description": "pretty",
          "text": "source /home/radxa/gcc/install/share/gcc-15.1.0/python/libstdcxx/v6/printers.py"
        },
        {
          "text": "add-auto-load-safe-path /home/radxa/gcc/install/lib64/libstdc++.so.6.0.34-gdb.py"
        }
      ]
    }
  ]
}